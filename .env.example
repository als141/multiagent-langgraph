# OpenAI API Configuration
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4o-mini
OPENAI_TEMPERATURE=0.7
OPENAI_MAX_TOKENS=1000

# LangGraph Configuration
LANGGRAPH_TRACING=true
LANGGRAPH_DEBUG=false

# Multi-Agent System Configuration
MAX_AGENTS=10
SIMULATION_ROUNDS=100
COOPERATION_THRESHOLD=0.6
MUTATION_RATE=0.1

# Game Theory Parameters
COOPERATION_REWARD=3
MUTUAL_COOPERATION_REWARD=3
MUTUAL_DEFECTION_PENALTY=1
BETRAYAL_REWARD=5
BETRAYAL_PENALTY=0

# Knowledge Evolution Settings
KNOWLEDGE_EXCHANGE_PROBABILITY=0.8
LEARNING_RATE=0.1
MEMORY_CAPACITY=50

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/multiagent_system.log

# Experimental Settings
RANDOM_SEED=42
ENABLE_VISUALIZATION=true
SAVE_RESULTS=true
RESULTS_DIR=results

# Development Settings
DEBUG_MODE=false
VERBOSE_LOGGING=false